#function docstring
def test7(a):
    """ this is my function for numeric for list"""
    l1=[]
    for i in a:
        if type(i) == list:
              for j in i:
                   l1.append(j)
        else:
            if type(i)== int or type(i)== float:
                l1.append(i)
    return l1
